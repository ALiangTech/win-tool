{
  "type": "module",
  "version": "0.0.1",
  "private": false,
  "packageManager": "pnpm@10.12.4",
  "scripts": {
    "dev": "vite --open",
    "preview": "vite preview",
    "build": "vite build",
    "build:test": "vite build -m test",
    "lint": "eslint .",
    "test": "vitest",
    "test:e2e": "cypress open",
    "typecheck": "vue-tsc --noEmit",
    "up": "taze major -I",
    "postinstall": "npx simple-git-hooks"
